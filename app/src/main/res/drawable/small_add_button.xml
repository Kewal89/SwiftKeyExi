<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android"  >
    <item>
     <shape android:shape="rectangle" >
         <corners
             android:radius="100dp"
             />
         <solid
             android:color="?attr/colorAccent"
             />
         <size
             android:width="100dp"
             android:height="100dp"
             />
     </shape>
    </item>
    <item
        android:top="20dp"
        android:right="45dp"
        android:bottom="20dp"
        android:left="45dp"
        >
        <shape android:shape="rectangle" >
            <solid
                android:color="#FFFFFF"
                />
        </shape>
    </item>
    <item
        android:top="45dp"
        android:right="20dp"
        android:bottom="45dp"
        android:left="20dp"
        >
        <shape android:shape="rectangle" >
            <solid
                android:color="#FFFFFF"
                />
        </shape>
    </item>
</layer-list>

